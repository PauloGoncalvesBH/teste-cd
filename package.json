{
  "name": "teste-continuous-delivery",
  "version": "1.0.0",
  "scripts": {
    "lint": "npx eslint src/ && npx eslint test/",
    "lint:fix": "npx eslint --fix src/ && npx eslint --fix test/",
    "webdriver-update": "webdriver-manager update --gecko false",
    "test:e2e": "npm run webdriver-update && protractor test/e2e/protractor.conf.js",
    "test:unit": "jasmine test/spec/*.spec.js",
    "test": "npm run test:unit && npm run test:e2e",
    "prettify": "prettier --write **/*.js **/*.md package.json sampleApp/**/* --print-width 120",
    "commit": "git-cz",
    "release": "standard-version -t"
  },
  "description": "Write robust and flakiness free tests with Protractor",
  "main": "index.js",
  "keywords": [
    "protractor",
    "e2e",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PauloGoncalvesBH/teste-cd"
  },
  "author": "Paulo Gon√ßalves <paulorochag@hotmail.com> (https://www.linkedin.com/in/paulo-goncalves)",
  "files": [
    "src"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "license": "MIT",
  "peerDependencies": {
    "protractor": ">= 5.0.0 < 6"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "husky": "^4.2.0",
    "jasmine": "^3.4.0",
    "prettier": "1.15.2",
    "protractor": "^5.4.2",
    "standard-version": "^7.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
